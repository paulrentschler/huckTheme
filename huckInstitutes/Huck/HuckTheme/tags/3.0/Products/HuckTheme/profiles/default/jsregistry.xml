<?xml version="1.0"?>
<!-- This file holds the setup configuration for the portal_javascripts tool.
     -->

<object name="portal_javascripts">
 <!-- Javascript elements accept these parameters:
      - 'id' (required): same rules as for stylesheets (see 'cssregistry.xml').
      - 'expression' (optional - default: ''): a tal condition.
      - 'inline' (optional - default: False)
      - 'enabled' (optional - default: True): boolean
      - 'cookable' (optional - default: True): boolean (aka 'merging allowed')
      -->

  <javascript id="webcam.js"
              cacheable="False"
              compression="safe" 
              cookable="False"
              enabled="True" 
              expression="python:here.id=='millennium-science-complex'" 
              inline="False" 
              />

  <javascript id="eventdates.js"
              cacheable="True" 
              compression="safe" 
              cookable="True"
              enabled="True" 
              expression="" 
              inline="False" 
              />
              
  <javascript id="setdates.js"
              cacheable="True" 
              compression="safe" 
              cookable="True"
              enabled="True" 
              expression="" 
              inline="False" 
              />

  <javascript id="criteriaui.js"
              cacheable="True" 
              compression="safe" 
              cookable="True"
              enabled="True" 
              expression="" 
              inline="False" 
              />

  <javascript id="highlight-rotator.js"
              cacheable="True" 
              compression="safe" 
              cookable="True"
              enabled="True" 
              expression="" 
              inline="False" 
              />

</object>