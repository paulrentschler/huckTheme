<?xml version="1.0"?>
<!-- This file holds the setup configuration for the portal_skins tool -->
<object name="portal_skins" allow_any="False" cookie_persistence="False"
        default_skin="hucktheme08" request_varname="plone_skin">

	<object name="hucktheme08_custom_images"
		meta_type="Filesystem Directory View"
		directory="Products.hucktheme08:skins/hucktheme08_custom_images"/>
	<object name="hucktheme08_styles"
		meta_type="Filesystem Directory View"
		directory="Products.hucktheme08:skins/hucktheme08_styles"/>
	<object name="hucktheme08_custom_templates"
		meta_type="Filesystem Directory View"
		directory="Products.hucktheme08:skins/hucktheme08_custom_templates"/>
	
	<skin-path name="hucktheme08" based-on="Plone Default">

		<layer name="hucktheme08_styles"
			 insert-after="custom"/>
		<layer name="hucktheme08_custom_images"
			 insert-after="hucktheme08_styles"/>
		<layer name="hucktheme08_custom_templates"
			 insert-after="hucktheme08_custom_images"/>
	</skin-path>
</object>